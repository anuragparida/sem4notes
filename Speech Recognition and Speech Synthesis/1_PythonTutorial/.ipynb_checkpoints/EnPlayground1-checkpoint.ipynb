    "import numpy as np\n",
    "\n",
    "class CCircle(object):\n",
    "    def __init__(self):\n",
    "        self.__radius = 0\n",
    "\n",
    "    def EvalArea(self):\n",
    "        return np.pi*(self.__radius**2)\n",
    "\n",
    "    def SetRadius(self, NewRadius):\n",
    "        assert NewRadius >= 0.0, 'NewRadius >= 0'\n",
    "        self.__radius = NewRadius"
